<% @toothbrush.each do |toothbrush| %>
  <li class="list-group-item d-flex justify-content-between align-items-center py-2 px-3 
            rounded-md text-sm text-dark bg-light border border-secondary cursor-pointer" 
            role="option" data-autocomplete-value="<%= toothbrush.id %>" data-autocomplete-label="<%= toothbrush.name %>">
    <%= toothbrush.name %>
  </li>
<% end %>
