<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>タイトル</th>
      <th>メーカー</th>
      <th>概要</th>
      <th></th>
      <th colspan="4"></th>
    </tr>
  </thead>
  <tbody>
    <% toothbrushes.each do |toothbrush| %>
      <div>
        <tr>
          <%= toothbrush.name %>
          <%= toothbrush.price %>
        </tr>
      </div>
      <%= image_tag (toothbrush['smallImageUrls'][0]) %>
    <% end %>
  </tbody>
</table>